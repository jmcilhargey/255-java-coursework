import javax.swing.*;
import java.util.*;

public class TestStringTokenizer {

	public static void main(String[] args) {
		String input;
		String token;
		String delim = ",";
		double d;
		input = JOptionPane.showInputDialog("Enter data separated by comma and space");
		
		StringTokenizer inputString = new StringTokenizer(input);
		while (inputString.hasMoreTokens()) {
			token = inputString.nextToken();
			
			JOptionPane.showMessageDialog(null, token);
			d = Double.parseDouble(token);
		}
		
	}

}
