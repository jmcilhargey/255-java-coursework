
public class Manager extends Employee {

	private double pctBonus;
	private double travelExpense;
	
	Manager(String n, double s, double p, double t) {
		super(n, s);
		pctBonus = p;
		travelExpense = t;
	}
	
	double getPctBonus() {
		return pctBonus;
	}
	
	double computeBonus() {
		return getSalary() * pctBonus;
	}
}
